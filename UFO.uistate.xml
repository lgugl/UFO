<?xml version="1.0" encoding="utf-8" ?>
<c2uistate>
    <!--This is a UI state file - its content describes the user interface settings when the project was last open.
It is entirely optional and the project will load without it.  If you are placing the project under source control,
you probably do not want to add any .uistate.xml files to the repository.-->
    <configurations>
        <active>
            <configuration name="HTML5" />
        </active>
        <display name="HTML5" />
    </configurations>
    <open-tabs>
        <layout>Title</layout>
        <layout>Game</layout>
        <layout>Menu</layout>
        <layout>End Game</layout>
        <layout>Options</layout>
        <event-sheet>Game Event sheet</event-sheet>
        <event-sheet>Title Event sheet</event-sheet>
        <event-sheet>Global Event sheet</event-sheet>
        <event-sheet>Menu Event sheet</event-sheet>
        <event-sheet>End Game Event sheet</event-sheet>
        <event-sheet active="1">Options Event sheet</event-sheet>
    </open-tabs>
    <properties>
        <property expanded="1" name="About" />
        <property expanded="1" name="Project settings" />
        <property expanded="0" name="Window Size" />
        <property expanded="1" name="Configuration Settings" />
    </properties>
    <families-expanded root="1">
        <family expanded="0" name="Bullets" />
        <family expanded="1" name="Enemies" />
        <family expanded="0" name="Weapons" />
        <family expanded="1" name="Menu" />
    </families-expanded>
</c2uistate>
